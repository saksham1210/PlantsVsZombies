<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="800.0" prefWidth="1500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.mainController">
   <children>
      <ImageView fitHeight="800.0" fitWidth="1500.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <image>
            <Image url="@../images/start.png" />
         </image>
      </ImageView>
      <VBox layoutX="550.0" layoutY="225.0" prefHeight="247.0" prefWidth="210.0" spacing="10.0" AnchorPane.bottomAnchor="332.0" AnchorPane.leftAnchor="594.0" AnchorPane.rightAnchor="696.0" AnchorPane.topAnchor="221.0">
         <children>
            <Button fx:id="startButton" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#startNewGame" prefHeight="50.0" prefWidth="250.0" style="-fx-background-color: #dcffcc;" text="START NEW GAME" textFill="#1578a5">
               <font>
                  <Font name="Gadugi Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="loadButton" alignment="CENTER" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onMouseClicked="#loadGame" prefHeight="50.0" prefWidth="250.0" style="-fx-background-color: #dcffcc;" text="LOAD GAME" textFill="#1578a5">
               <font>
                  <Font name="Gadugi Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="chooseLevelButton" alignment="CENTER" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#chooseLevel" prefHeight="50.0" prefWidth="250.0" style="-fx-background-color: #dcffcc;" text="CHOOSE LEVEL" textFill="#1578a5">
               <font>
                  <Font name="Gadugi Bold" size="18.0" />
               </font>
            </Button>
            <Button alignment="CENTER" layoutX="10.0" layoutY="45.0" mnemonicParsing="false" onMouseClicked="#exitGame" prefHeight="50.0" prefWidth="250.0" style="-fx-background-color: #dcffcc;" text="EXIT" textFill="#1578a5">
               <font>
                  <Font name="Gadugi Bold" size="18.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
